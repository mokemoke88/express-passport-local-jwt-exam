{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": true,
        "trailingComma": "es5"
      }
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
